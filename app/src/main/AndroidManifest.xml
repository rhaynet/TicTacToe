<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.tictactoe">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".versus_human_activity"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".versus_computer_activity"
            android:configChanges="orientation"
            android:screenOrientation="portrait" />
        <activity android:name=".board_size_3x3_versus_comp"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".board_size_5x5_versus_comp"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".board_size_3x3_versus_human"
            android:configChanges="orientation"
            android:screenOrientation="portrait"/>
        <activity android:name=".board_size_5x5_versus_human"
            android:configChanges="orientation"
            android:screenOrientation="portrait">
        </activity>
    </application>

</manifest>